window.addEventListener('load', ()=> {
const number1 = document.querySelectorAll('.counter_number.cc-integer')
const number2 = document.querySelectorAll('.counter_number.cc-decimal')
const number3 = document.querySelectorAll('.counter_number.cc-comma')
// we will save the animations in a timline
let tl = gsap.timeline({
paused: true,
defaults: { duration: 3, textContent: 0 },
scrollTrigger: {
trigger: '.sc_b--numbers.sc_b',
start: 'top 75%',
}
})
/* Fist block */
// normal count up animation:
tl.from(number1, {
// start from 0
snap: { textContent: 1 }, // increment by 1
}, 0) // insert 0sec into the timeline
/* Second block */
// count up animation with decimals:
tl.from(number2, {
snap: { textContent: 0.1 }, // increment by 0.1
}, 0)
/* Third block */
// count up animation with commas (your initial numbers shouldn't have commas):
tl.from(number3, {
snap: { textContent: 1 },
onUpdate: function () { // on every increment, format the number
number3.textContent = parseInt(number3.textContent).toLocaleString()
},
}, 0)

